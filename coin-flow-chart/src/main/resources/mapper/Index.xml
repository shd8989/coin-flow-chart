<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.crypto.db.mapper.IndexMapper">
    <!-- 
    <select id="selectDataList" parameterType="map" resultType="IndexEntity">
        match(v:person)
        return v.name
        limit 5
    </select>
     -->
    
    <select id="selectDataList" resultType="IndexEntity">
        match(v:person)
        return v.name
        limit 5
    </select>
    
    <insert id="updateCoinAllData" parameterType="Map">
        insert into tb_coin_detail
		(
			market
			,trade_date
			,trade_time
			,trade_date_kst
			,trade_time_kst
			,trade_timestamp
			,opening_price
			,high_price
			,low_price
			,trade_price
			,prev_closing_price
			,change
			,change_price
			,change_rate
			,signed_change_price
			,signed_change_rate
			,trade_volume
			,acc_trade_price
			,acc_trade_price_24h
			,acc_trade_volume
			,acc_trade_volume_24h
			,highest_52_week_price
			,highest_52_week_date
			,lowest_52_week_price
			,lowest_52_week_date
			,timestamp
		)
		values
		<foreach collection="list" item="item" separator=",">
		(
			#{item.market}
			,#{item.trade_date}
			,#{item.trade_time}
			,#{item.trade_date_kst}
			,#{item.trade_time_kst}
			,#{item.trade_timestamp}
			,#{item.opening_price}
			,#{item.high_price}
			,#{item.low_price}
			,#{item.trade_price}
			,#{item.prev_closing_price}
			,#{item.change}
			,#{item.change_price}
			,#{item.change_rate}
			,#{item.signed_change_price}
			,#{item.signed_change_rate}
			,#{item.trade_volume}
			,#{item.acc_trade_price}
			,#{item.acc_trade_price_24h}
			,#{item.acc_trade_volume}
			,#{item.acc_trade_volume_24h}
			,#{item.highest_52_week_price}
			,#{item.highest_52_week_date}
			,#{item.lowest_52_week_price}
			,#{item.lowest_52_week_date}
			,now()
		)
		</foreach>
    </insert>
</mapper>